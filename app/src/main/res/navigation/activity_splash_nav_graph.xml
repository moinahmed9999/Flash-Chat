<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_splash_nav_graph"
    app:startDestination="@id/splashLogoFragment">

    <fragment
        android:id="@+id/splashLogoFragment"
        android:name="com.moin.flashchat.ui.fragment.SplashLogoFragment"
        android:label="fragment_splash_logo"
        tools:layout="@layout/fragment_splash_logo" >
        <action
            android:id="@+id/action_splashLogoFragment_to_signInActivity"
            app:destination="@id/signInActivity" />
    </fragment>
    <activity
        android:id="@+id/signInActivity"
        android:name="com.moin.flashchat.ui.activity.SignInActivity"
        android:label="activity_sign_in"
        tools:layout="@layout/activity_sign_in" />
</navigation>